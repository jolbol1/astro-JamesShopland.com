{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		}
	],
	"images": {
		"domains": [
			""
		],
		"sizes": [
			16,
			32,
			48,
			64,
			96,
			128,
			256,
			384,
			640,
			750,
			828,
			1080,
			1200,
			1920,
			2048,
			3840
		],
		"remotePatterns": []
	}
}